{
  "name": "@wandeber/agnese",
  "version": "0.6.6-alpha",
  "description": "This project will help you to map data easily based on a configuration in JSON/YAML.",
  "license": "CC BY-ND 4.0",
  "author": "Bernardo A. Siverio [wandeber] <berthewanderer@gmail.com> (https://wandeber.com)",
  "homepage": "https://github.com/wandeber/agnese",
  "repository": {
    "type": "git",
    "url": "https://github.com/wandeber/agnese"
  },
  "bugs": {
    "url": "https://github.com/wandeber/agnese/issues"
  },
  "keywords": [
    "map",
    "map data",
    "mapper",
    "mapping",
    "mappings",
    "data mapper",
    "data-mapper",
    "object mapper",
    "object-mapper",
    "map array",
    "loop",
    "conditions",
    "conditional",
    "if",
    "iterate",
    "array",
    "json",
    "yaml",
    "yml",
    "schema",
    "open",
    "open source",
    "agnese"
  ],
  "type": "module",
  "main": "./dist/Agnese.cjs",
  "module": "./dist/Agnese.js",
  "types": "./dist/Agnese.d.ts",
  "exports": {
    "import": "./dist/Agnese.js",
    "require": "./dist/Agnese.cjs"
  },
  "files": [
    "dist/**/*",
    "other-dependencies/**/*",
    "LICENSE",
    "README.md",
    "schema/**/*"
  ],
  "dependencies": {
    "quara": "0.10.13-alpha"
  },
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/node": "^20.12.7",
    "@typescript-eslint/eslint-plugin": "^7.7.0",
    "@typescript-eslint/parser": "^7.7.0",
    "eslint": "^8.57.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.2",
    "tsup": "^8.5.1",
    "typescript": "^5.4.5"
  },
  "optionalDependencies": {
    "yaml": "^2.2.2"
  },
  "scripts": {
    "rebuild": "npm run clean && npm run build",
    "build": "npm run clean && npm run build:tsup",
    "build-prod": "npm run build && npm run check",
    "preversion": "npm run check",
    "check": "npm test && npm run lint",
    "clean": "rm -rf ./dist && rm -rf ./dist-npm",
    "build:tsup": "tsup --config tsup.config.cjs",
    "lint": "npx eslint ./src/**/*.ts",
    "test": "npx jest --detectOpenHandles --forceExit --coverage --verbose",
    "gen-dist-npm": "./scripts/generate-dist-npm.sh",
    "test-module-import": "node ./scripts/test-module-import.cjs"
  },
  "engines": {
    "node": ">=18.17.1",
    "npm": ">=9.6.7"
  }
}