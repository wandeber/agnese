  "repository": {
    "type": "git",
    "url": "https://github.com/wandeber/agnese.git"
  },
  "bugs": {
    "url": "https://github.com/wandeber/agnese/issues"
  },
  "keywords": [
    "map",
    "map data",
    "mapper",
    "mapping",
    "mappings",
    "data mapper",
    "data-mapper",
    "object mapper",
    "object-mapper",
    "map array",
    "loop",
    "conditions",
    "conditional",
    "if",
    "iterate",
    "array",
    "json",
    "yaml",
    "yml",
    "schema",
    "open",
    "open source",
    "agnese"
  ],
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "files": [
    "dist/**/*",
    "other-dependencies/**/*",
    "schema/**/*"
  ],
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.1",
    "@typescript-eslint/eslint-plugin": "^7.2.0",
    "@typescript-eslint/parser": "^7.2.0",
    "eslint": "^8.57.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.2",
    "typescript": "^5.4.2"
  },
  "optionalDependencies": {
    "yaml": "^2.2.2"
  },
  "scripts": {
    "rebuild": "npm run clean && npm run build",
    "build": "npm run build-tsc",
